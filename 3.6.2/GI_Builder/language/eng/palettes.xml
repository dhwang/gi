<data jsxid="jsxroot">
	<record jsxid="p_dom" jsxtext="Component Hierarchy" jsxkeycode="ctrl+1"
		jsxexecute="jsx3.ide.doSelectPalette(this, strRECORDID)"
		idename="jsx_ide_props_dom"
		ideurl="components/dom/dom.xml"
		idedefault="q2" project="1" config="true"/>
		
	<record jsxid="p_props" jsxdivider="1" jsxtext="Properties Editor" jsxkeycode="ctrl+2"
		jsxexecute="jsx3.ide.doSelectPalette(this, strRECORDID)"
		idename="jsx_ide_props_contentblock"
		ideurl="components/properties/properties.xml"
		idedefault="q4" project="1" config="true"/>
	<record jsxid="p_events" jsxtext="Events Editor" jsxkeycode="ctrl+3" jsxtip="edit/update events that execute in 'model context'"
		jsxexecute="jsx3.ide.doSelectPalette(this, strRECORDID)"
		idename="jsx_ide_me_contentblock"
		ideurl="components/modelevents/modelevents.xml"
		idedefault="q4" project="1" config="true"/>
	<record jsxid="p_attributes" jsxtext="Attributes Editor" jsxkeycode="ctrl+4" jsxtip="edit/update attributes to extend the HTML tags (the 'view') generated by the local page server"
		jsxexecute="jsx3.ide.doSelectPalette(this, strRECORDID)"
		idename="jsx_ide_attributes_contentblock"
		ideurl="components/attributes/attributes.xml"
		idedefault="q4" idedefaultclosed="1" project="1" config="true"/>
	<record jsxid="p_xslparams" jsxtext="XSL Parameters" jsxkeycode="ctrl+5" jsxtip=""
		jsxexecute="jsx3.ide.doSelectPalette(this, strRECORDID)"
		idename="jsx_ide_xslparams_contentblock"
		ideurl="components/xslparams/xslparams.xml"
		idedefault="q4" idedefaultclosed="1" project="1" config="true"/>
		
	<record jsxid="p_lib" jsxdivider="1" jsxtext="Component Libraries" jsxkeycode="ctrl+6"
		jsxexecute="jsx3.ide.doSelectPalette(this, strRECORDID)"
		idename="jsx_ide_palette_complibs"
		ideurl="components/componentlibs/componentlibs.xml"
		idedefault="q3" project="1" config="true"/>
	<record jsxid="p_cache" jsxtext="Local Data Cache" jsxkeycode="ctrl+7"
		jsxexecute="jsx3.ide.doSelectPalette(this, strRECORDID)"
		idename="jsx_ide_cache_contentblock"
		ideurl="components/cache/cache.xml"
		idedefault="q2" project="1" config="true"/>
	<record jsxid="p_files" jsxtext="Project Files" jsxkeycode="ctrl+8"
		jsxexecute="jsx3.ide.doSelectPalette(this, strRECORDID)"
		idename="jsx_ide_resources_contentblock"
		ideurl="components/resourcefiles/resourcefiles.xml"
		idedefault="q1" project="1" config="true"/>

	<record jsxid="d_trash" jsxdivider="1" jsxtext="Recycle Bin" jsxkeycode="ctrl+9"
	  jsxexecute="jsx3.ide.doToggleRecycleBin();"
		idename="jsx_ide_recycling_bin" project="1" config="true"/>
	<record jsxid="d_sysout" jsxtext="System Log" jsxkeycode="ctrl+l"
	  jsxexecute="jsx3.ide.toggleSystemOut();"
		idename="jsx_ide_systemout" config="true"/>
</data>
