<serialization xmlns="urn:tibco.com/v3.0" >
 <name><![CDATA[]]></name>
 <icon><![CDATA[]]></icon>
 <description><![CDATA[]]></description>
 <onBeforeDeserialize><![CDATA[]]></onBeforeDeserialize>
 <onAfterDeserialize><![CDATA[

objJSX.doExecute = function(objEvent) {
  this.getDescendantOfName('jsx_tbb_execute').doExecute(objEvent);
};

objJSX.doClear = function() {
  this.getDescendantOfName('jsx_txt_scriptlet_input').setValue("", true);
};

objJSX.doMinimize = function() {
  this.doToggleState(jsx3.gui.Dialog.MINIMIZED);
};

objJSX.onFocus = function() {
  var me = this;
  window.setTimeout(function(){
    me.getDescendantOfName('jsx_txt_scriptlet_input').focus();
  }, 0);
};

objJSX.registerHotKey('doExecute', jsx3.gui.Event.KEY_ENTER, false, true, false); // enter
objJSX.registerHotKey('doClear', 'k', false, false, true); // clear on alt-k
objJSX.registerHotKey('doMinimize', 'm', false, true, false); // minimize on ctrl-m
objJSX.registerHotKey('doClose', 'w', false, false, true); // close on alt-w

// The JSX debugger is only supported in IE
if (! jsx3.CLASS_LOADER.IE) {
  var btn = objJSX.getDescendantOfName("jsx_tbb_step");
  btn.setEnabled(jsx3.gui.Form.STATEDISABLED);
  btn.applyDynamicProperties();
  btn.setTip(btn.getTip() + " (IE Only)");
  btn.setDynamicProperty("jsxtip", null);
}

 ]]></onAfterDeserialize>
