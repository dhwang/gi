<serialization xmlns="urn:tibco.com/v3.0">
	<name><![CDATA[]]></name>
	<icon><![CDATA[]]></icon>
	<description><![CDATA[]]></description>
	<onBeforeDeserialize><![CDATA[]]></onBeforeDeserialize>
	<onAfterDeserialize><![CDATA[
	
objJSX.doMinimize = function() {
	this.doToggleState(jsx3.gui.Dialog.MINIMIZED);
};

objJSX.doCloseMe = function() {
	jsx3.ide.doMovePalette(this.getDescendantOfName('jsxmenu_dock'), 'close');
};

objJSX.registerHotKey('doMinimize', 'm', false, true, false); // minimize on ctrl-m
objJSX.registerHotKey('doCloseMe', 'w', false, false, true); // close on alt-w

	]]></onAfterDeserialize>
		<object type="jsx3.gui.Dialog">
			<dynamics jsxbgcolor="@Solid Medium" />
			<variants jsxwidth="300" jsxheight="300" />
			<strings jsxname="jsx_ide_palette_dialog" jsxvisibility="" />
			<events jsxaftermove="this.constrainPosition(); jsx3.ide.paletteDidMove(this);" jsxafterresize="jsx3.ide.paletteDidMove(this);"/>
				<object type="jsx3.gui.WindowBar">
					<variants />
					<strings jsxname="abc_cbar" jsxtext="Palette Title" />
						<object type="jsx3.gui.ToolbarButton">
							<dynamics jsximage="@Min Icon" jsxtip="jsx3.gui.Dialog.min"/>
							<variants />
							<strings jsxname="jsx_tbb" />
							<events jsxexecute="this.getAncestorOfType(jsx3.gui.Dialog).doToggleState();" />
						</object>
						<object type="jsx3.gui.Menu">
							<dynamics jsxxmlurl="mnuDocking" />
							<variants jsxtype="0"/>
							<strings jsxname="jsxmenu_dock" jsxtip="docking options" jsximage="images/icon_81.gif" jsxmargin="2 5 0 0"/>
							<events jsxmenu="this.selectItem('float');" />
						</object>
				</object>
		</object>
</serialization>
