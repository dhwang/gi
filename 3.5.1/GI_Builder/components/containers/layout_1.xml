objJSX.onStartup = function() {
  //set project name on upper toolbar
  var appath = jsx3.IDE.getJSXByName("jsxactiveprojectpath");
  var project = jsx3.ide.SERVER;

  if (appath && project) {
    var projectUrl = jsx3.ide.getActiveProjectDirectory().toURI();
    appath.setText('<nobr><a target="_new" style="color:darkblue;" href="' + projectUrl + '">' +
        project.getRelPath() + '</a></nobr>', true);
  } else if (appath) {
    appath.getParent().removeChild(appath);
  }
}

jsx3.IDE.subscribe(jsx3.ide.events.STARTUP, objJSX, "onStartup");

jsx3.ide.loadCustomHotKeys();

jsx3.ide.adjustSplittersOnStartup(); // doing this before paint() avoids some box recalcs

