objJSX.onInit = function() {
  var path = jsx3.ide.SERVER.getAppPath();
  var xhtml = jsx3.util.strEndsWith(jsx3.app.Browser.getLocation().getPath().toLowerCase(), ".xhtml");
  var shell = "shell" + (xhtml ? ".xhtml" : ".html");
  var uri = jsx3.net.URI.fromParts(null, null, null, null, shell, "jsxapppath=" + path, null);

  var c = this.getDescendantOfName('inlinedivcontent');
  c.setValue(c.getValue().replace('@APPPATH@', path));
  c = this.getDescendantOfName('launchlinkcontent');
  c.setValue(c.getValue().replace('@SHELLPATH@', uri.toString()));
};

objJSX.onBrowse = function() {
  jsx3.require("jsx3.io.FileDialog");

  var fDialog = jsx3.io.FileDialog.deserialize(this.getServer().getRootBlock(), "lupb", true);

  var me = this;
  fDialog.onExecute = function(objFile) {
    me.getDescendantOfName('launchpageurl').setValue(jsx3.ide.relativePathTo(objFile));
  };

  var baseFolder = null;
  var pageUri = this.getDescendantOfName('launchpageurl').getValue();
  if (pageUri) {
    var currentFile = jsx3.ide.getSystemRelativeFile(pageUri);
    if (currentFile.exists()) {
      baseFolder = currentFile.getParentFile();
    } else if (pageUri.indexOf('/') >= 0) {
      var folderUri = pageUri.substring(0, pageUri.lastIndexOf('/'));
      baseFolder = jsx3.ide.getSystemRelativeFile(folderUri);
      if (! baseFolder.isDirectory()) baseFolder = null;
    }
  }

  if (baseFolder == null)
    baseFolder = jsx3.ide.getSystemDirFile();

  fDialog.openForSave(baseFolder, baseFolder.getRootDirectory());
};

objJSX.onCreate = function() {
  var pageUri = this.getDescendantOfName('launchpageurl').getValue();
  if (pageUri) {
    var outFile = jsx3.ide.getSystemRelativeFile(pageUri);
    var bXHTML = this.getDescendantOfName("xhtmlbox").getChecked() == jsx3.gui.CheckBox.CHECKED;
    jsx3.ide.createLaunchPage(outFile, bXHTML);
    this.doClose();
  } else {
    this.beep();
  }
};

objJSX.doMinimize = function() {
  this.doToggleState(jsx3.gui.Dialog.MINIMIZED);
};

objJSX.registerHotKey('doMinimize', 'm', false, true, false); // minimize on ctrl-m
objJSX.registerHotKey('doClose', 'w', false, false, true); // close on alt-w
objJSX.getDescendantOfName('tab_launchpage').registerHotKey(
  function() {this.getAncestorOfType(jsx3.gui.Dialog).onCreate();},
  jsx3.gui.Event.KEY_ENTER, false, false, false); // save on return key

objJSX.onHelp = function() {
  var tp = this.getDescendantOfName('tabbedpane');
  var tab = tp.getChild(tp.getSelectedIndex()) || this;
  this.getServer().invokeHelp(tab);
};

objJSX.onInit();

 ]]></onAfterDeserialize>
