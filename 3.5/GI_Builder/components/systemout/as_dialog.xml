							<strings jsxname="jsxmenu_sysoutdock" jsxxmlid="mnuSystemOutDocking" jsximage="images/icon_81.gif" jsxmargin="2 5 0 0" />
							<events jsxmenu="this.selectItem('float');" />
						</object>
            <object type="jsx3.gui.Menu">
