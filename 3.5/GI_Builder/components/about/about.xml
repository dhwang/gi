    </object>
    <object type="jsx3.gui.Block">
     <dynamics />
     <variants jsxwidth="700" jsxrelativeposition="0" jsxleft="10" jsxtop="50"/>
     <strings jsxname="jsx_about_copy"/>
    </object>
  </object>
