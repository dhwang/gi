objJSX.doActivate = function() {
  jsx3.sleep(this._doActivate, "jsx3.ide.palette.properties", this, true);
};

objJSX.doDeactivate = function() {
  jsx3.sleep(this._doDeactivate, "jsx3.ide.palette.properties", this, true);
};

objJSX._doActivate = function() {
  if (jsx3.ide.getSelected().length > 0) {
    jsx3.ide.onPropertiesChange();
//    this.getDescendantOfName('jsxproperties').setDisplay(jsx3.gui.Block.DISPLAYBLOCK, true);
  }
};

objJSX._doDeactivate = function() {
  this.getDescendantOfName('jsxproperties').setDisplay(jsx3.gui.Block.DISPLAYNONE, true);
};

objJSX.doCleanup = function() {
  jsx3.IDE.unsubscribe(jsx3.ide.events.COMPONENT_EDITOR_DID_ACTIVATE, this);
  jsx3.IDE.unsubscribe(jsx3.ide.events.COMPONENT_EDITOR_DID_DEACTIVATE, this);
};

objJSX.makeSpy = function(objGrid, strRecordId) {
  var node = objGrid.getRecordNode(strRecordId);
  if (node == null) return null;
  if (node.getAttribute("group") == "1") return null;

  var desc = node.getAttribute("jsxtip");
  var getter = node.getAttribute("docgetter");
  var setter = node.getAttribute("docsetter");
  var deflt = node.getAttribute("docdefault");

  var dep = node.getAttribute("deprecated");

  var strHTML = "";
  strHTML += "<span class='jsx3ide_propsspy'>";

  strHTML += "<div class='name" + (dep ? " deprecated" : "") + "'>" + node.getAttribute("jsxtext") + "</div>";

  if (dep)
    strHTML += "<div class='desc dep'><span class='title'>Deprecated.</span> " + (dep != "1" ? dep : "") + "</div>";

  strHTML += "<div class='desc'>" + (desc || "<i>No description provided.</i>") + "</div>";
  if (deflt)
    strHTML += "<div class='deflt'><span class='title'>Default Value:</span> " + deflt + "</div>";
  if (getter)
    strHTML += "<div class='method'><span class='title'>Getter:</span> " + getter + "()</div>";
  if (setter)
    strHTML += "<div class='method'><span class='title'>Setter:</span> " + setter + "()</div>";
  if (! node.getAttribute("docnoprop"))
    strHTML += "<div class='prop'><span class='title'>Property:</span> " + node.getAttribute("jsxid") + "</div>";

  strHTML += "</span>";
  return strHTML;
};

jsx3.IDE.subscribe(jsx3.ide.events.COMPONENT_EDITOR_DID_ACTIVATE, objJSX, "doActivate");
jsx3.IDE.subscribe(jsx3.ide.events.COMPONENT_EDITOR_DID_DEACTIVATE, objJSX, "doDeactivate");

var pathMask = objJSX.getDescendantOfName("jsxpath");
pathMask.maskBrowse = function() {
  jsx3.require("jsx3.io.FileDialog");

  var baseFile = jsx3.ide.getCurrentUserHome();
  var dirFile = jsx3.ide.getCurrentDirectory();
  var val = this.getMaskValue();
  if (val) {
    var fileURI = jsx3.ide.SERVER.resolveURI(val);
    if (fileURI.getScheme() == null || fileURI.getScheme() == "file") {
      try {
        var chosenFile = jsx3.ide.getSystemRelativeFile(jsx3.ide.SERVER.resolveURI(val));
        var parentFile = new jsx3.io.File(chosenFile.getParentPath());
        if (parentFile.isDirectory()) {
          dirFile = parentFile;
          if (!(parentFile.equals(baseFile) || parentFile.isDescendantOf(baseFile)))
            baseFile = parentFile.getRootDirectory();
        }
      } catch (e) {}
    }
  }

  this.suspendEditSession();
  var fd = jsx3.io.FileDialog.deserialize(jsx3.IDE.getRootBlock(), "props_mask", true);

  var me = this;
  fd.onExecute = function(objFile) {
    var jsxDir = jsx3.ide.getSystemRelativeFile("JSX");
    var ideDir = jsx3.ide.getBuilderRelativeFile(".");
    var newVal = null;

    // special handling for JSX/ and GI_Builder/
    if (objFile.isDescendantOf(jsxDir)) {
      newVal = jsx3.net.URIResolver.JSX.relativizeURI(jsx3.ide.getSystemDirFile().toURI().relativize(objFile.toURI()));
    } else if (objFile.isDescendantOf(ideDir)) {
      newVal = jsx3.ide.getSystemDirFile().toURI().relativize(objFile.toURI());
    } else {
      newVal = jsx3.ide.SERVER.getBaseDirectory().relativePathTo(objFile);
    }

    me.setMaskValue(newVal);
    jsx3.ide.setCurrentDirectory(objFile.getParentFile());
    me.getDescendantOfName("btnBrowse").focus();
    me.resumeEditSession();
  };

  fd.onCancel = function() {
    me.getDescendantOfName("btnBrowse").focus();
    me.resumeEditSession();
  };

  fd.openForOpen(dirFile, baseFile, jsx3.io.FileDialog.CHOOSE_FILE);
};

var colorMask = objJSX.getDescendantOfName("jsxcolor");
colorMask.maskOpen = function() {
  jsx3.ide.openColorPickerMask(this);
};

var cdfMask = objJSX.getDescendantOfName("jsxcdf");
cdfMask.maskOpen = function() {
  jsx3.ide.openCdfMask(this);
};

if (jsx3.ide.getSelected().length > 0) {
  jsx3.ide.onPropertiesChange();
  objJSX.doActivate();
} else {
  objJSX.doDeactivate();
}

