<serialization xmlns="urn:tibco.com/v3.0">
  <onAfterDeserialize><![CDATA[
objJSX.setInfo = function(jid, nick) {
  var user_text = '';
  if(jid == nick){
    user_text = jid;
  } else {
    user_text = nick + ' (' + jid + ')';
  }
  nick_block.setText('Really delete ' + user_text + ' from your roster?');
  this.jid = jid;
};
objJSX.getJID = function() {
  return textbox.getValue();
};
  ]]></onAfterDeserialize>
