<serialization xmlns="urn:tibco.com/v3.0">
 <name><![CDATA[Dialog]]></name>
 <icon><![CDATA[]]></icon>
 <description><![CDATA[Dialog box.  (Set Modal property for a modal dialog).]]></description>
 <onBeforeDeserialize><![CDATA[]]></onBeforeDeserialize>
 <onAfterDeserialize><![CDATA[
 
objJSX.openProject = function() {
  var objSelect = this.getDescendantOfName('projectSelect');
  var objRecord = objSelect.getRecord(objSelect.getValue());
  if (objRecord && objRecord.jsxid) {
    jsx3.ide.doOpenProject(objRecord.jsxid);
  }
};
 
objJSX.populateProjectMenu = function(objMenu, strRecordId) {
  jsx3.ide.populateProjectMenu(objMenu, strRecordId || "userproj");
};

objJSX.onInit = function() {
};
 
objJSX.onInit();

 ]]></onAfterDeserialize>
  <object type="jsx3.gui.Dialog">
   <dynamics jsxbgcolor="@Solid Medium"/>
   <variants jsxwindowstate="1" jsxresize="0"/>
   <strings jsxname="dialog" jsxwidth="400" jsxheight="150"/>
    <object type="jsx3.gui.WindowBar">
     <variants/>
     <strings jsxname="caption bar" jsxtext="Open/Create Project"/>
      <object type="jsx3.gui.ToolbarButton">
       <dynamics jsximage="@Min Icon" jsxtip="@Min Tip"/>
       <variants/>
       <strings jsxname="min button"/>
       <events jsxexecute="this.getParent().getParent().doToggleState();"/>
      </object>
    </object>
    <object type="jsx3.gui.LayoutGrid">
     <dynamics jsxbgcolor="@Solid Medium"/>
     <variants jsxrelativeposition="0" jsxleft="0" jsxtop="0" jsxoverflow="2"/>
     <strings jsxname="layout (--)" jsxwidth="100%" jsxheight="100%" jsxrows="60,*"/>
      <object type="jsx3.gui.Block">
       <dynamics jsxpadding="@8Pixel"/>
       <variants jsxoverflow="2"/>
       <strings jsxname="pane" jsxwidth="100%" jsxheight="100%" jsxtext="Welcome to @gi.productname.full.esc@. To get started choose from the following options."/>
      </object>
      <object type="jsx3.gui.Block">
       <variants jsxoverflow="1"/>
       <strings jsxname="pane" jsxwidth="100%" jsxheight="100%"/>
        <object type="jsx3.gui.LayoutGrid">
         <variants jsxrelativeposition="0" jsxleft="0" jsxtop="0" jsxoverflow="2"/>
         <strings jsxname="layout ( | )" jsxwidth="100%" jsxheight="100%" jsxcols="170,33,*"/>
          <object type="jsx3.gui.Block">
           <variants jsxoverflow="2"/>
           <strings jsxname="pane" jsxwidth="100%" jsxheight="100%" jsxtextalign="right"/>
            <object type="jsx3.gui.Button">
             <variants jsxindex="0" jsxheight="18"/>
             <strings jsxname="button" jsxtext="Create New Project" jsxmargin="3 0 0 0"/>
             <events jsxexecute="jsx3.ide.newProject();"/>
            </object>
          </object>
          <object type="jsx3.gui.Block">
           <variants jsxoverflow="1"/>
           <strings jsxname="pane" jsxwidth="100%" jsxheight="100%" jsxtext="or" jsxtextalign="center" jsxpadding="5 0 0 0"/>
          </object>
          <object type="jsx3.gui.Block">
           <variants jsxoverflow="1"/>
           <strings jsxname="pane" jsxwidth="100%" jsxheight="100%" jsxtextalign="left"/>
            <object type="jsx3.gui.Menu">
             <variants jsxwidth="150" />
             <strings jsxxml="&lt;data jsxid=&quot;jsxroot&quot;/&gt;" jsxname="projectSelect" jsxtext="Open Existing Project..." jsxstyleoverride="background-color:white;"/>
             <events jsxmenu="this.getAncestorOfType(jsx3.gui.Dialog).populateProjectMenu(this, strPARENTID);" />
            </object>
          </object>
        </object>
      </object>
    </object>
  </object>
</serialization>
