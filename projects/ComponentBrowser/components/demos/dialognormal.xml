        <strings jsxname="minimize" jsxmargin="0 4 4 0" jsxxml="&lt;data jsxid=&quot;jsxroot&quot;&gt;&lt;record jsxid=&quot;0&quot; jsxtext=&quot;Hide&quot;/&gt;&lt;record jsxid=&quot;1&quot; jsxtext=&quot;Show&quot;/&gt;&lt;/data&gt;"/>
        <events jsxselect="root.onMinimizeButtonSelect(strRECORDID);"/>
