<serialization xmlns="urn:tibco.com/v3.0" jsxversion="3.9">
  <name><![CDATA[HTML Output]]></name>
  <icon/>
  <description><![CDATA[This shows a BlockX component doing HTML output from XML source.]]></description>
  <onBeforeDeserialize/>
  <onAfterDeserialize><![CDATA[
desc.setText(root.getMetaValue("description"));
name.setText(root.getMetaValue("name"));
  ]]></onAfterDeserialize>
  <object type="jsx3.gui.LayoutGrid">
    <variants jsxrelativeposition="0" jsxleft="0" jsxtop="0" jsxoverflow="2"/>
    <strings jsxname="root" jsxcols="*,200" jsxwidth="100%" jsxheight="100%"/>
    <events jsxdestroy="objPARENT.getServer().setLocale(jsx3.util.Locale.valueOf(''));"/>
    <object type="jsx3.gui.Block">
      <variants jsxoverflow="2"/>
      <strings jsxname="col1" jsxwidth="100%" jsxheight="100%"/>
      <dynamics jsxborder="@Outset"/>
      <object type="jsx3.gui.BlockX">
        <variants jsxrelativeposition="0" jsxwidth="300" jsxheight="100" jsxxmlasync="1" jsxxslasync="1"/>
        <strings jsxname="blockX" jsxxml="&lt;helloworld&gt;Hello, World!&lt;/helloworld&gt;"/>
        <dynamics jsxbgcolor="@Solid Light" jsxpadding="@8Pixel"/>
      </object>
    </object>
    <object type="jsx3.gui.Block">
      <variants jsxoverflow="1"/>
      <strings jsxname="col2" jsxwidth="100%" jsxheight="100%"/>
      <dynamics jsxbgcolor="@Col2 Background" jsxpadding="@8Pixel"/>
      <object type="jsx3.gui.Block">
        <variants jsxfontsize="12"/>
        <strings jsxname="name" jsxwidth="100%" jsxtext="HTML Output" jsxfontweight="bold" jsxpadding="0 0 4 0"/>
      </object>
      <object type="jsx3.gui.Block">
        <variants/>
        <strings jsxname="desc" jsxwidth="100%" jsxtext="This shows a BlockX component doing HTML output from XML source."/>
      </object>
    </object>
  </object>
</serialization>
