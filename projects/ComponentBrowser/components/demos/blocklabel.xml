<serialization xmlns="urn:tibco.com/v3.0" jsxversion="3.9">
  <name><![CDATA[Label]]></name>
  <icon/>
  <description><![CDATA[This shows some properties you can set on a label.]]></description>
  <onBeforeDeserialize/>
  <onAfterDeserialize><![CDATA[
this.onWidthChange = function(strValue) {
  block.setWidth(strValue, true);
};

this.onHeightChange = function(strValue) {
  block.setHeight(strValue, true);
};

this.onTextChange = function(strValue) {
  block.setText(strValue, true);
};

this.onBackgroundColorChange = function(strValue) {
  block.setBackgroundColor(strValue, true);
};

this.onFontChange = function(strValue) {
  block.setFontName(strValue).repaint();
};

width.setValue(block.getWidth());
height.setValue(block.getHeight());
text.setValue(block.getText());
bgcolor.setValue(block.getBackgroundColor());
font.setValue(block.getFontName());
  ]]></onAfterDeserialize>
  <object type="jsx3.gui.LayoutGrid">
    <variants jsxrelativeposition="0" jsxleft="0" jsxtop="0" jsxoverflow="2"/>
    <strings jsxname="root" jsxcols="*,200" jsxwidth="100%" jsxheight="100%"/>
    <events/>
    <object type="jsx3.gui.Block">
      <variants jsxoverflow="2"/>
      <strings jsxname="col1" jsxwidth="100%" jsxheight="100%"/>
      <dynamics jsxborder="@Outset"/>
      <object type="jsx3.gui.Block">
        <variants jsxrelativeposition="0" jsxoverflow="3" jsxwidth="200" jsxleft="20" jsxtop="20"/>
        <strings jsxheight="14" jsxname="block" jsxtext="Lorem ipsum dolor sit amet." jsxbgcolor="#FFFFFF" jsxfontname="Verdana"/>
      </object>
    </object>
    <object type="jsx3.gui.Block">
      <variants jsxoverflow="1"/>
      <strings jsxname="col2" jsxwidth="100%" jsxheight="100%"/>
      <dynamics jsxbgcolor="@Col2 Background" jsxpadding="@8Pixel"/>
      <object type="jsx3.gui.Block">
        <variants jsxfontsize="12"/>
        <strings jsxname="name" jsxwidth="100%" jsxtext="Block Relative" jsxfontweight="bold" jsxpadding="0 0 4 0"/>
      </object>
      <object type="jsx3.gui.Block">
        <variants/>
        <strings jsxname="desc" jsxwidth="100%" jsxtext="This shows the properties you can set on a relative block."/>
      </object>
      <object type="jsx3.gui.Block">
        <variants/>
        <strings jsxname="hr" jsxwidth="100%" jsxtext="&lt;hr&gt;" jsxpadding="8 0 8 0"/>
      </object>
      <object type="jsx3.gui.Label">
        <variants jsxrelativeposition="1" jsxoverflow="3" jsxwidth="75"/>
        <strings jsxname="formLabel" jsxtext="Width" jsxfor="width" jsxfontweight="bold" jsxtextalign="left"/>
      </object>
      <object type="jsx3.gui.TextBox">
        <variants jsxheight="18" jsxwidth="100" jsxvalue="200"/>
        <strings jsxname="width" jsxmargin="0 4 4 0"/>
        <events jsxchange="root.onWidthChange(strVALUE);"/>
      </object>
      <object type="jsx3.gui.Label">
        <variants jsxrelativeposition="1" jsxoverflow="3" jsxwidth="75"/>
        <strings jsxname="formLabel" jsxtext="Height" jsxfor="height" jsxfontweight="bold" jsxtextalign="left"/>
      </object>
      <object type="jsx3.gui.TextBox">
        <variants jsxheight="18" jsxwidth="100"/>
        <strings jsxvalue="14" jsxname="height" jsxmargin="0 4 4 0"/>
        <events jsxchange="root.onHeightChange(strVALUE);"/>
      </object>
      <object type="jsx3.gui.Label">
        <variants jsxrelativeposition="1" jsxoverflow="3" jsxwidth="75"/>
        <strings jsxname="formLabel" jsxtext="Text" jsxfor="text" jsxfontweight="bold" jsxtextalign="left"/>
      </object>
      <object type="jsx3.gui.TextBox">
        <variants jsxheight="18" jsxwidth="100"/>
        <strings jsxvalue="Lorem ipsum dolor sit amet." jsxname="text" jsxmargin="0 4 4 0"/>
        <events jsxchange="root.onTextChange(strVALUE);"/>
      </object>
      <object type="jsx3.gui.Label">
        <variants jsxrelativeposition="1" jsxoverflow="3" jsxwidth="75"/>
        <strings jsxname="formLabel" jsxtext="BG Color" jsxfor="bgcolor" jsxfontweight="bold" jsxtextalign="left"/>
      </object>
      <object type="jsx3.gui.TextBox">
        <variants jsxheight="18" jsxwidth="100"/>
        <strings jsxvalue="#FFFFFF" jsxname="bgcolor" jsxmargin="0 4 4 0"/>
        <events jsxchange="root.onBackgroundColorChange(strVALUE);"/>
      </object>
      <object type="jsx3.gui.Label">
        <variants jsxrelativeposition="1" jsxoverflow="3" jsxwidth="75"/>
        <strings jsxname="formLabel" jsxtext="Font" jsxfor="font" jsxfontweight="bold" jsxtextalign="left"/>
      </object>
      <object type="jsx3.gui.TextBox">
        <variants jsxheight="18" jsxwidth="100"/>
        <strings jsxname="font" jsxmargin="0 4 4 0"/>
        <events jsxchange="root.onFontChange(strVALUE);"/>
      </object>
    </object>
  </object>
</serialization>
