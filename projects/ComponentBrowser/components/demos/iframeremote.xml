<serialization xmlns="urn:tibco.com/v3.0" jsxversion="3.9">
  <name><![CDATA[IFrame (Remote)]]></name>
  <icon/>
  <description><![CDATA[This demonstrates a remotely hosted IFrame.]]></description>
  <onBeforeDeserialize/>
  <onAfterDeserialize><![CDATA[
this.onUrlChange = function(strValue) {
  iframe.setSrc(strValue);
};

this.onScrollingSelect = function(strRecordId) {
  iframe.setScrolling(strRecordId).repaint();
};

url.setValue(iframe.getSrc());
scrolling.setValue(iframe.getScrolling());
  ]]></onAfterDeserialize>
  <object type="jsx3.gui.LayoutGrid">
    <variants jsxrelativeposition="0" jsxleft="0" jsxtop="0" jsxoverflow="2"/>
    <strings jsxname="root" jsxcols="*,200" jsxwidth="100%" jsxheight="100%"/>
    <events/>
    <object type="jsx3.gui.Block">
      <variants jsxoverflow="2"/>
      <strings jsxname="col1" jsxwidth="100%" jsxheight="100%"/>
      <dynamics jsxborder="@Outset"/>
      <object type="jsx3.gui.IFrame">
        <variants jsxscroll="1"/>
        <strings jsxname="iframe" jsxwidth="100%" jsxheight="100%" jsxsrc="http://www.generalinterface.org/"/>
      </object>
    </object>
    <object type="jsx3.gui.Block">
      <variants jsxoverflow="1"/>
      <strings jsxname="col2" jsxwidth="100%" jsxheight="100%"/>
      <dynamics jsxbgcolor="@Col2 Background" jsxpadding="@8Pixel"/>
      <object type="jsx3.gui.Block">
        <variants jsxfontsize="12"/>
        <strings jsxname="name" jsxwidth="100%" jsxfontweight="bold" jsxpadding="0 0 4 0" jsxtext="IFrame (Remote)"/>
      </object>
      <object type="jsx3.gui.Block">
        <variants/>
        <strings jsxname="desc" jsxwidth="100%" jsxtext="This demonstrates a remotely hosted IFrame."/>
      </object>
      <object type="jsx3.gui.Block">
        <variants/>
        <strings jsxname="hr" jsxwidth="100%" jsxtext="&lt;hr&gt;" jsxpadding="8 0 8 0"/>
      </object>
      <object type="jsx3.gui.Label">
        <variants jsxrelativeposition="1" jsxoverflow="3" jsxwidth="75"/>
        <strings jsxname="formLabel" jsxtext="URL" jsxfor="url" jsxfontweight="bold" jsxtextalign="left"/>
      </object>
      <object type="jsx3.gui.TextBox">
        <variants jsxheight="18" jsxwidth="100"/>
        <strings jsxname="url" jsxmargin="0 4 4 0" jsxvalue="http://www.generalinterface.org/"/>
        <events jsxchange="root.onUrlChange(strVALUE);"/>
      </object>
      <object type="jsx3.gui.Label">
        <variants jsxrelativeposition="1" jsxoverflow="3" jsxwidth="75"/>
        <strings jsxname="formLabel" jsxtext="Scrolling" jsxfor="scrolling" jsxfontweight="bold" jsxtextalign="left"/>
      </object>
      <object type="jsx3.gui.Select">
        <variants jsxwidth="100" jsxheight="18" jsxxmlasync="1"/>
        <strings jsxname="scrolling" jsxmargin="0 4 4 0" jsxxml="&lt;data jsxid=&quot;jsxroot&quot;&gt;&lt;record jsxid=&quot;1&quot; jsxtext=&quot;Yes&quot;/&gt;&lt;record jsxid=&quot;2&quot; jsxtext=&quot;No&quot;/&gt;&lt;record jsxid=&quot;3&quot; jsxtext=&quot;Auto&quot;/&gt;&lt;/data&gt;"/>
        <events jsxselect="root.onScrollingSelect(strRECORDID);"/>
      </object>
    </object>
  </object>
</serialization>
