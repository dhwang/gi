<serialization xmlns="urn:tibco.com/v3.0" jsxversion="3.9">
  <name><![CDATA[Button]]></name>
  <icon/>
  <description><![CDATA[This shows some properties you can set on a button.]]></description>
  <onBeforeDeserialize/>
  <onAfterDeserialize/>
  <object type="jsx3.gui.LayoutGrid">
    <variants jsxrelativeposition="0" jsxleft="0" jsxtop="0" jsxoverflow="2"/>
    <strings jsxname="root" jsxcols="*,200" jsxwidth="100%" jsxheight="100%"/>
    <events jsxdestroy="objPARENT.getServer().setLocale(jsx3.util.Locale.valueOf(''));"/>
    <object type="jsx3.gui.Block">
      <variants jsxoverflow="2"/>
      <strings jsxname="col1" jsxwidth="100%" jsxheight="100%"/>
      <dynamics jsxborder="@Outset"/>
      <object type="jsx3.gui.StackGroup">
        <variants jsxoverflow="2" jsxbarsize="27"/>
        <strings jsxname="stackgroup" jsxrows="*,27,27" jsxbgcolor="#FFFFFF"/>
        <object type="jsx3.gui.Stack">
          <variants/>
          <strings jsxname="stack1" jsxtext="Stack 1"/>
          <dynamics jsxactivecolor="@Solid Medium AC" jsxinactivecolor="@Solid Disabled IC"/>
          <object type="jsx3.gui.Block">
            <variants jsxoverflow="1" jsxloadtype="4"/>
            <strings jsxname="pane" jsxbgcolor="#ffffff" jsxdisplay="" jsxtext="Pane 1 text" jsxpadding="8"/>
          </object>
          <object type="jsx3.gui.Menu">
            <variants jsxxmlasync="1"/>
            <strings jsxname="menu" jsxtip="menu" jsxtext="Menu 1" jsxxml="&lt;data jsxid=&quot;jsxroot&quot;&gt;  &lt;record jsxid=&quot;1&quot; jsxtext=&quot;New&quot;/&gt;  &lt;record jsxid=&quot;2&quot; jsxtext=&quot;Open&quot;/&gt;  &lt;record jsxid=&quot;3&quot; jsxtext=&quot;Save&quot;/&gt;  &lt;/data&gt;"/>
          </object>
        </object>
        <object type="jsx3.gui.Stack">
          <variants/>
          <strings jsxname="stack2" jsxtext="Stack 2"/>
          <dynamics jsxactivecolor="@Solid Medium AC" jsxinactivecolor="@Solid Disabled IC"/>
          <object type="jsx3.gui.Block">
            <variants jsxoverflow="1" jsxloadtype="4"/>
            <strings jsxname="pane" jsxbgcolor="#ffffff" jsxdisplay="none" jsxtext="Pane 2 text" jsxpadding="8"/>
          </object>
          <object type="jsx3.gui.Menu">
            <variants jsxxmlasync="1"/>
            <strings jsxname="menu" jsxtip="menu" jsxtext="Menu 2" jsxxml="&lt;data jsxid=&quot;jsxroot&quot;&gt;  &lt;record jsxid=&quot;1&quot; jsxtext=&quot;New&quot;/&gt;  &lt;record jsxid=&quot;2&quot; jsxtext=&quot;Open&quot;/&gt;  &lt;record jsxid=&quot;3&quot; jsxtext=&quot;Save&quot;/&gt;  &lt;/data&gt;"/>
          </object>
        </object>
        <object type="jsx3.gui.Stack">
          <variants/>
          <strings jsxname="stack3" jsxtext="Stack 3"/>
          <dynamics jsxactivecolor="@Solid Medium AC" jsxinactivecolor="@Solid Disabled IC"/>
          <object type="jsx3.gui.Block">
            <variants jsxoverflow="1" jsxloadtype="4"/>
            <strings jsxname="pane" jsxbgcolor="#ffffff" jsxdisplay="none" jsxtext="Pane 3 text" jsxpadding="8"/>
          </object>
          <object type="jsx3.gui.Menu">
            <variants jsxxmlasync="1"/>
            <strings jsxname="menu" jsxtip="menu" jsxtext="Menu 3" jsxxml="&lt;data jsxid=&quot;jsxroot&quot;&gt;  &lt;record jsxid=&quot;1&quot; jsxtext=&quot;New&quot;/&gt;  &lt;record jsxid=&quot;2&quot; jsxtext=&quot;Open&quot;/&gt;  &lt;record jsxid=&quot;3&quot; jsxtext=&quot;Save&quot;/&gt;  &lt;/data&gt;"/>
          </object>
        </object>
      </object>
    </object>
    <object type="jsx3.gui.Block">
      <variants jsxoverflow="1"/>
      <strings jsxname="col2" jsxwidth="100%" jsxheight="100%"/>
      <dynamics jsxbgcolor="@Col2 Background" jsxpadding="@8Pixel"/>
      <object type="jsx3.gui.Block">
        <variants jsxfontsize="12"/>
        <strings jsxname="name" jsxwidth="100%" jsxfontweight="bold" jsxpadding="0 0 4 0"/>
      </object>
      <object type="jsx3.gui.Block">
        <variants/>
        <strings jsxname="desc" jsxwidth="100%"/>
      </object>
    </object>
  </object>
</serialization>
