<serialization xmlns="urn:tibco.com/v3.0" jsxversion="3.9">
  <properties xmlns="">
    <data jsxid="jsxroot">
      <record jsxid="jsxrgb" jsxtext="Color"
          jsxtip="Sets the RGB of the selected color."
