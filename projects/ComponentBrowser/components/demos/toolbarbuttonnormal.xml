<serialization xmlns="urn:tibco.com/v3.0" jsxversion="3.9">
  <name><![CDATA[Toolbar Button]]></name>
  <icon/>
  <description><![CDATA[This demonstrates various ways to set up toolbar buttons.]]></description>
  <onBeforeDeserialize/>
  <onAfterDeserialize/>
  <object type="jsx3.gui.LayoutGrid">
    <variants jsxrelativeposition="0" jsxleft="0" jsxtop="0" jsxoverflow="2"/>
    <strings jsxname="root" jsxwidth="100%" jsxheight="100%"/>
    <dynamics jsxcols="@Root Cols"/>
    <object type="jsx3.gui.Block">
      <variants jsxoverflow="2"/>
      <strings jsxname="col1" jsxwidth="100%" jsxheight="100%"/>
      <dynamics jsxborder="@Outset"/>
      <object type="jsx3.gui.WindowBar">
        <variants jsxbartype="1"/>
        <strings jsxname="toolbar"/>
        <object type="jsx3.gui.ToolbarButton">
          <variants jsxenabled="1"/>
          <strings jsxname="tbOpen" jsximage="jsx:///images/tbb/open.gif" jsxtext="Open"/>
          <events jsxexecute="window.alert('button clicked');"/>
        </object>
        <object type="jsx3.gui.ToolbarButton">
          <variants jsxenabled="1"/>
          <strings jsxname="tbDelete" jsximage="jsx:///images/list/delete.gif" jsxtext="Delete"/>
          <events jsxexecute="window.alert('button clicked');"/>
        </object>
        <object type="jsx3.gui.ToolbarButton">
          <variants jsxenabled="1"/>
          <strings jsxname="tbNext" jsximage="jsx:///images/matrix/arrow.gif" jsxtext="Next"/>
          <events jsxexecute="window.alert('button clicked');"/>
        </object>
        <object type="jsx3.gui.ToolbarButton">
          <variants jsxenabled="1" jsxdivider="1"/>
          <strings jsxname="tbProperties" jsxtext="Properties"/>
          <events jsxexecute="window.alert('button clicked');"/>
        </object>
        <object type="jsx3.gui.ToolbarButton">
          <variants jsxenabled="0" jsxdivider="1"/>
          <strings jsxname="tbInfo" jsximage="jsx:///images/select/selected.gif" jsxtext="Info"/>
          <events jsxexecute="window.alert('button clicked');"/>
        </object>
      </object>
    </object>
    <object type="jsx3.gui.Block">
      <variants jsxoverflow="1"/>
      <strings jsxname="col2" jsxwidth="100%" jsxheight="100%"/>
      <dynamics jsxbgcolor="@Col2 Background" jsxpadding="@8Pixel"/>
      <object type="jsx3.gui.Block">
        <variants jsxfontsize="12"/>
        <strings jsxname="name" jsxwidth="100%" jsxfontweight="bold" jsxpadding="0 0 4 0"/>
      </object>
      <object type="jsx3.gui.Block">
        <variants/>
        <strings jsxname="desc" jsxwidth="100%"/>
      </object>
      <object type="jsx3.gui.Block">
        <variants jsxoverflow="3"/>
        <strings jsxname="properties" jsxwidth="100%" jsxrelativeposition="1"/>
      </object>
    </object>
  </object>
</serialization>