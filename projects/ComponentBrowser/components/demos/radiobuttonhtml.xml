<serialization xmlns="urn:tibco.com/v3.0" jsxversion="3.9">
  <name><![CDATA[HTML Label Radio Button]]></name>
  <icon/>
  <description><![CDATA[This demonstrates HTML in radio button labels.]]></description>
  <onBeforeDeserialize/>
  <onAfterDeserialize/>
  <object type="jsx3.gui.LayoutGrid">
    <variants jsxrelativeposition="0" jsxleft="0" jsxtop="0" jsxoverflow="2"/>
    <strings jsxname="root" jsxwidth="100%" jsxheight="100%"/>
    <dynamics jsxcols="@Root Cols"/>
    <object type="jsx3.gui.Block">
      <variants jsxoverflow="2"/>
      <strings jsxname="col1" jsxwidth="100%" jsxheight="100%"/>
      <dynamics jsxborder="@Outset" jsxpadding="@8Pixel"/>
      <object type="jsx3.gui.RadioButton">
        <variants jsxheight="18" jsxdefaultselected="1" jsxselected="1" jsxrelativeposition="1"/>
        <strings jsxname="radio1" jsxtext="Radio button 1, with &lt;b&gt;bold&lt;/b&gt; and &lt;i&gt;italic&lt;/i&gt; content" jsxgroupname="group1" jsxmargin="0 4 0 0" jsxstyleoverride="display:block"/>
      </object>
      <object type="jsx3.gui.RadioButton">
        <variants jsxheight="18" jsxdefaultselected="1" jsxselected="0" jsxrelativeposition="1"/>
        <strings jsxname="radio2" jsxtext="&lt;span style='color:red;'&gt;Radio button 2&lt;/span&gt;, now with colors!" jsxgroupname="group1" jsxmargin="0 4 0 0" jsxstyleoverride="display:block"/>
      </object>
    </object>
    <object type="jsx3.gui.Block">
      <variants jsxoverflow="1"/>
      <strings jsxname="col2" jsxwidth="100%" jsxheight="100%"/>
      <dynamics jsxbgcolor="@Col2 Background" jsxpadding="@8Pixel"/>
      <object type="jsx3.gui.Block">
        <variants jsxfontsize="12"/>
        <strings jsxname="name" jsxwidth="100%" jsxfontweight="bold" jsxpadding="0 0 4 0"/>
      </object>
      <object type="jsx3.gui.Block">
        <variants/>
        <strings jsxname="desc" jsxwidth="100%"/>
      </object>
    </object>
  </object>
</serialization>
