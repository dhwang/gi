<serialization xmlns="urn:tibco.com/v3.0" jsxversion="3.9">
  <name><![CDATA[Toggling ImageButton]]></name>
  <icon/>
  <description><![CDATA[This demonstrates a toggling ImageButton. Click the button repeatedly to see messages showing the current state.]]></description>
  <onBeforeDeserialize/>
  <onAfterDeserialize/>
  <object type="jsx3.gui.LayoutGrid">
    <variants jsxrelativeposition="0" jsxleft="0" jsxtop="0" jsxoverflow="2"/>
    <strings jsxname="root" jsxcols="*,200" jsxwidth="100%" jsxheight="100%"/>
    <events/>
    <object type="jsx3.gui.Block">
      <variants jsxoverflow="2" jsxpadding="8"/>
      <strings jsxname="col1" jsxwidth="100%" jsxheight="100%"/>
      <dynamics jsxborder="@Outset"/>
      <object type="jsx3.gui.ImageButton">
        <variants jsxwidth="16" jsxheight="16" jsxenabled="1" jsxprefetch="1" jsxtype="1" jsxstate="0"/>
        <strings jsxname="imagebutton" jsxtext="Example ImageButton" jsximage="jsx:///images/imagebutton/off.gif" jsxoverimage="jsx:///images/imagebutton/over.gif" jsxdownimage="jsx:///images/imagebutton/down.gif" jsxonimage="jsx:///images/imagebutton/on.gif" jsxdisabledimage="jsx:///images/imagebutton/disabled.gif"/>
        <events jsxtoggle="status.setText(&quot;The toggle state is now &quot; + intSTATE, true);"/>
      </object>
      <object type="jsx3.gui.Block">
        <strings jsxname="status" jsxmargin="0 0 0 10"/>
      </object>
    </object>
    <object type="jsx3.gui.Block">
      <variants jsxoverflow="1"/>
      <strings jsxname="col2" jsxwidth="100%" jsxheight="100%"/>
      <dynamics jsxbgcolor="@Col2 Background" jsxpadding="@8Pixel"/>
      <object type="jsx3.gui.Block">
        <variants jsxfontsize="12"/>
        <strings jsxname="name" jsxwidth="100%" jsxfontweight="bold" jsxpadding="0 0 4 0"/>
      </object>
      <object type="jsx3.gui.Block">
        <variants/>
        <strings jsxname="desc" jsxwidth="100%"/>
      </object>
    </object>
  </object>
</serialization>
