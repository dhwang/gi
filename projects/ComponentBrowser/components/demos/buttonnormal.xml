      <object type="jsx3.gui.TextBox">
        <variants jsxheight="18" jsxwidth="100"/>
        <strings jsxname="width" jsxmargin="0 4 4 0"/>
        <events jsxchange="root.onWidthChange(strVALUE);"/>
