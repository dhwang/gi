<?xml version="1.0"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee" version="2.5">
  
  <servlet>
    <servlet-name>jack</servlet-name>
    <servlet-class>org.jackjs.JackServlet</servlet-class>
    <init-param>
    	<param-name>narwhalFilePath</param-name>
    	<param-value>C:/dev/narwhal</param-value>
    </init-param>
    <init-param>
    	<param-name>modulesFilePath</param-name>
    	<param-value>C:/customers/tibco/prototype-library/WEB-INF/jslib</param-value>
    </init-param>
    
  </servlet>
  
  <servlet-mapping>
    <servlet-name>jack</servlet-name>
    <url-pattern>/data/*</url-pattern>
  </servlet-mapping>
  
</web-app>
